<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!--
            Sequences for LAB
        -->
        <record id="seq_lab_sub" model="ir.sequence">
            <field name="name">LAB</field>
            <field name="code">lab.sub</field>
            <field name="prefix">LAB/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!--
            Sequences for op
        -->
        <record id="pre_op_checklist_seq" model="ir.sequence">
            <field name="name">PRE OP</field>
            <field name="code">pre.op</field>
            <field name="prefix">PRE/OP/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="op_checklist_seq" model="ir.sequence">
            <field name="name">OP</field>
            <field name="code">op.check</field>
            <field name="prefix">OP/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="prod_op_checklist_seq" model="ir.sequence">
            <field name="name">PROD OP</field>
            <field name="code">prod.op</field>
            <field name="prefix">PROD/OP/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!--
            Sequences for Restroom
        -->
        <record id="restroom_log_seq" model="ir.sequence">
            <field name="name">restroom log</field>
            <field name="code">restroom.log</field>
            <field name="prefix">RML/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!--
            Sequences for supplier management
        -->
        <record id="supplier_management_seq" model="ir.sequence">
            <field name="name">supplier management</field>
            <field name="code">supplier.management</field>
            <field name="prefix">SM/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!--
            Sequences for foreign material
        -->
        <record id="foreign_material_seq" model="ir.sequence">
            <field name="name">foreign material</field>
            <field name="code">foreign.material</field>
            <field name="prefix">FM/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>

</odoo>
